[flake8]
max-line-length = 88
extend-ignore = 
    E203,
    E501,
    W503,
    E402,
    F401,
    F403,
    F405,
    C901,
    E722,
    F541,
    F841,
    E712,
    E266,
    E741,
    F811
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    .eggs,
    *.egg,
    .pytest_cache,
    .mypy_cache,
    docs,
    data
max-complexity = 10
per-file-ignores =
    __init__.py:F401
    tests/*:S101
    */generated/*:E501
    util/util.py:C901
